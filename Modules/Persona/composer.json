{
    "name": "zaghloul-soft/persona-module",
    "description": "Persona Decoupled Module HMVC Repository Based",
    "type": "laravel-module",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "Abdallah Zaghloul",
            "email": "3bdallahzaghloul@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Nwidart\\Modules\\LaravelModulesServiceProvider",
                "Prettus\\Repository\\Providers\\RepositoryServiceProvider"
            ],
            "aliases": {}
        },
        "module-dir": "Modules"
    },
    "autoload": {
        "psr-4": {
            "Modules\\Persona\\": ""
